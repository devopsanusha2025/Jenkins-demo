pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Code checkout from GitHub'
            }
        }

        stage('Build') {
            steps {
                echo 'Build started'
            }
        }

        stage('Test') {
            steps {
                echo 'Testing application'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying application'
            }
        }
    }
}

